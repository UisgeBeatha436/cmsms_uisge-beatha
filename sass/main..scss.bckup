  /* ---------- list items horizontal -----------*/
  .list-group-horizontal {
    display: table;
    width: 100%;
    table-layout: fixed;
    border-collapse: separate;
    &>.list-group-item {
      border: 1px solid #808080;
      display: table-cell;
      float: none;
      text-align: center;
      width: 1%;
      border-radius: 0;

      &:first-child {
        border-bottom-left-radius: 3px;
      }

      &:last-child {
        border-bottom-right-radius: 3px;
      }
    }
  }

